#########################################
# Snowflake Environment Configuration
# Rename this file to `.env` and fill in your values
#########################################

# Your full Snowflake account name.
# Example: "yourcompany-abc123" or "xy12345.us-east-1"
SNOWFLAKE_ACCOUNT= [fill-here]

# Your Snowflake username.
# Example: "JDOE"
SNOWFLAKE_USER=your_snowflake_username

# Your Snowflake password.
# DO NOT commit your actual password â€” use a secure secrets manager in production.
SNOWFLAKE_PASSWORD=your_snowflake_password

# Role to assume for the session.
# Example: "SYSADMIN"
SNOWFLAKE_ROLE=[fill-here]

# Warehouse to use for query execution.
# Example: "COMPUTE_WH"
SNOWFLAKE_WAREHOUSE=[fill-here]

# Target database in Snowflake.
# Example: "MY_DB"
SNOWFLAKE_DATABASE=[fill-here]

# Schema within the specified database.
# Example: "PUBLIC"
SNOWFLAKE_SCHEMA=[fill-here]


